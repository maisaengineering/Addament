<div id="show_todo">
       <p id="notice"><%= notice %></p><br><br>
<table>
	<tr>
		<td>
<label>Title:</label></td>
<td><%= @todo.title %><br></td>
</tr>
<tr>
<td>
<label>Description:</label></td>
<td>
<%= @todo.description %><br></td>
</tr>
<tr>
<td>	
<label>Event Date:</label></td>
<td>
<%= @todo.event_date.to_date.strftime("%B %d, %Y") %><br></td>
</tr>
<tr>
<td>	
<label>Time:</label></td>

<td><%= @todo.start_time.strftime("%I:%M%p") %> to
<%= @todo.end_time.strftime("%I:%M%p") %></td>
</tr>
</table>

<!--<%= link_to 'Back', root_url %>-->
</div>